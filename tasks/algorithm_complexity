Написать сложности алгоритмов по времени в O-нотации

1.
for (int i = 0; i < n; ++i) { // B O(n^2)
  for (int j = 1; j < m; ++j) { // A O(n)
    a[i][j]++; // С O(1)
  }
}

2.
for (int i = 0; i < n; ++i) { // B O(n*log(n))
  for (int j = 1; j < n; j *= 2) { // A O(log(n))
    a[i][j]++; 
  }
}

3.
for (int i = 1; i < n; ++i) { // B O(n^2)
  for (int j = 0; j < n/i; ++j) { // A O(n)
    matrx[i][j]++;
  }
}